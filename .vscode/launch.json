{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Easy Project",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/easy-project/server.js",
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "restart": true,
      "runtimeExecutable": "node",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
